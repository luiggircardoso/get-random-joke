{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "random-jokes",
  "title": "Random Jokes",
  "description": "A Raycast extension that provides random jokes to brighten your day.",
  "icon": "icon.png",
  "author": "luiggircardoso",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "get-random-joke",
      "title": "Get Random Joke",
      "description": "Fetch a random joke.",
      "mode": "view"
    }
  ],
  "main": "index.js",
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luiggircardoso/get-random-joke.git"
  },
  "keywords": [],
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/luiggircardoso/get-random-joke/issues"
  },
  "homepage": "https://github.com/luiggircardoso/get-random-joke#readme",
  "dependencies": {
    "@raycast/api": "^1.100.3",
    "@raycast/eslint-config": "^2.0.4",
    "@raycast/utils": "^2.2.0",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "title-case": "^4.3.2",
    "typescript": "^5.8.3"
  }
}
